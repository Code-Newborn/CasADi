{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Configure MSVC Debug",
            "type": "process",
            "command": "D:/CMake/bin/cmake.exe",
            "args": [
                "-S",
                ".",
                "-B",
                "build_msvc_debug",
                "-G",
                "Visual Studio 17 2022",
                "-A",
                "x64",
                "-DvsCasadiRoot=${config:vsCasadiRoot}"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": []
        },
        {
            "label": "Configure MSVC Release",
            "type": "process",
            "command": "D:/CMake/bin/cmake.exe",
            "args": [
                "-S",
                ".",
                "-B",
                "build_msvc_release",
                "-G",
                "Visual Studio 17 2022",
                "-A",
                "x64",
                "-DvsCasadiRoot=${config:vsCasadiRoot}"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": []
        },
        {
            "label": "Build MSVC Debug",
            "type": "process",
            "command": "D:/CMake/bin/cmake.exe",
            "args": [
                "--build",
                "build_msvc_debug",
                "--config",
                "Debug"
            ],
            "options": {
                "cwd": "${workspaceFolder}",
                "env": {
                    "PATH": "${config:vsCasadiBin}\\Debug;${env:PATH}"
                }
            },
            "dependsOn": [
                "Configure MSVC Debug"
            ],
            "dependsOrder": "sequence",
            "problemMatcher": []
        },
        {
            "label": "Build MSVC Release",
            "type": "process",
            "command": "D:/CMake/bin/cmake.exe",
            "args": [
                "--build",
                "build_msvc_release",
                "--config",
                "Release"
            ],
            "options": {
                "cwd": "${workspaceFolder}",
                "env": {
                    "PATH": "${config:vsCasadiBin}\\Release;${env:PATH}"
                }
            },
            "dependsOn": [
                "Configure MSVC Release"
            ],
            "dependsOrder": "sequence",
            "problemMatcher": []
        }
    ]
}